<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#00000000"
    android:orientation="vertical">
    <Button
        android:id="@+id/btn_general"
        style="@style/CouponButtonStyle"
        android:text="通用券"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_groupon"
        style="@style/CouponButtonStyle"
        android:text="团购券"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_discount"
        style="@style/CouponButtonStyle"
        android:text="折扣券"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_gift"
        style="@style/CouponButtonStyle"
        android:text="礼品券"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_cash"
        style="@style/CouponButtonStyle"
        android:text="代金券"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_member_card"
        style="@style/CouponButtonStyle"
        android:text="会员卡"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_scenic_ticket"
        style="@style/CouponButtonStyle"
        android:text="门票"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_movie_ticket"
        style="@style/CouponButtonStyle"
        android:text="电影票"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_boarding_pass"
        style="@style/CouponButtonStyle"
        android:text="飞机票"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/btn_lucky_money"
        style="@style/CouponButtonStyle"
        android:text="红包"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/sheetTextcolor" />

    <Button
        android:id="@+id/cancel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:background="@drawable/pop_bottom_selector"
        android:text="取消"
        android:textColor="@color/sheetTextClicked"
        android:textSize="@dimen/sp_16" />


</LinearLayout>
